PACKAGE=@PACKAGE@
PROG=@PROG@
VERSION=@VERSION@
BUGREPORT=@BUGREPORT@
URL=@URL@

CONFDIR=@CONFDIR@
EXECDIR=@EXECDIR@

WITH_MCEXPLORE = @WITH_MCEXPLORE@
WITH_C10T = @WITH_C10T@

SHELL=/bin/sh
VPATH=@srcdir@

# root for installation
prefix=@prefix@
exec_prefix=@exec_prefix@

# executables
bindir=@bindir@
sbindir=@sbindir@
libexecdir=@libexecdir@

# data
datarootdir=@datarootdir@
datadir=@datadir@
sysconfdir=@sysconfdir@
sharedstatedir=@sharedstatedir@
localstatedir=@localstatedir@

libdir=@libdir@
localedir=@localedir@
srcdir=@srcdir@

INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_DATA=@INSTALL_DATA@

all:

clean:

INSTALLDIRS = $(DESTDIR)${sysconfdir}/bash_completion.d $(DESTDIR)${CONFDIR}/action.d

installdirs:
	$(INSTALL) -d $(INSTALLDIRS)

install: installdirs all
	$(INSTALL_DATA) bash_completion $(DESTDIR)${sysconfdir}/bash_completion.d/$(PROG)

ifeq ($(WITH_C10T), yes)
	$(INSTALL_DATA) map.sh          $(DESTDIR)${CONFDIR}/action.d/map.sh
endif

ifeq ($(WITH_MCEXPLORE), yes)
	$(INSTALL_DATA) explore.sh      $(DESTDIR)${CONFDIR}/action.d/explore.sh
endif

uninstall:

dist:

distclean:

check:

info:

.PHONY: all clean installdirs install uninstall dist distclean check info

